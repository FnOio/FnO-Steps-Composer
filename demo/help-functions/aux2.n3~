@prefix : <http://example.org#>.
@prefix e: <http://eulersharp.sourceforge.net/2003/03swap/log-rules#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.

#the second element needs to be a list of graphs
{
(?preresult () ) :unifyList ?result.
}
<={

("!") e:derive true.
_:x e:trace ?preresult.
?preresult e:labelvars ?out.
_:x e:trace (?preresult ?out).
?out log:equalTo ?result.
?e e:trace (?result)
}.

{
(?graph1 ?graphlist ) :unifyList ?result.
}
<=
{
?graphlist rdf:first ?first.

?graphlist rdf:rest ?rest.

?graph1 e:graphMember ?first.
(?graph1 ?rest) :unifyList ?result.
("!") e:derive true.
}.

{
(?graph1 ?graphlist ) :unifyList ?result.
}
<=
{
?graphlist rdf:first ?first.

?graphlist rdf:rest ?rest.

_:x e:fail {?graph1 e:graphMember ?first}.
_:e e:trace ("hoho" ?first).
(?graph1 ?rest) :unifyList ?result.
("!") e:derive true.
}.


