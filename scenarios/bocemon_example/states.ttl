@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix o-address: <https://data.vlaanderen.be/ns/adres#> .
@prefix o-steps: <https://fast.ilabt.imec.be/ns/oslo-steps/0.2#> .
@prefix o-persoon: <https://data.vlaanderen.be/ns/persoon#> .
@prefix ex: <http://example.org/ns/example#> .
@prefix xl: <http://www.w3.org/2008/05/skos-xl#> .
@prefix locn: <http://www.w3.org/ns/locn#> .
@prefix sh:      <http://www.w3.org/ns/shacl#> .
@prefix schema: <https://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

@prefix state: <http://localhost:8000/states#>.
@prefix shape: <http://localhost:8000/shapes#>.
@prefix step: <http://localhost:8000/steps#>.

state:roomEquipedForHVACControl a o-steps:State;
  rdfs:label "The room is equiped for HVAC control"@en ;
  o-steps:hasStateShape shape:roomEquipedForHVACControlShape .

state:thermometerInstalled a o-steps:State ;
  rdfs:label "Room has a thermometer"@en ;
  o-steps:hasStateShape shape:thermometerInstalledShape .

state:humiditySensorInstalled a o-steps:State ;
  rdfs:label "Room has a humidity sensor."@en ;
  o-steps:hasStateShape shape:humiditySensorInstalledShape .

state:HVACConnected a o-steps:State ;
                              rdfs:label "Room connected to HVAC."@en ;
                              o-steps:hasStateShape shape:HVACConnectedShape .
