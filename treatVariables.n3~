@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix p-plan: <http://purl.org/net/p-plan#> .
@prefix o-address: <https://data.vlaanderen.be/ns/adres#> .
@prefix o-steps: <https://example.org/ns/oslo-steps#> .
@prefix ex: <https://example.org/ns/example#> .
@prefix var: <https://example.org/ns/var#> .
PREFIX log: <http://www.w3.org/2000/10/swap/log#>
PREFIX e: <http://eulersharp.sourceforge.net/2003/03swap/log-rules#>
PREFIX gps: <http://josd.github.io/eye/reasoning/gps/gps-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
@prefix : <https://example.org/ns/example#> .
PREFIX list: <http://www.w3.org/2000/10/swap/list#>

{
()  :produceSkolemfunctions (?skolem ?skolem)
}<={}.


{
?graphlist  :produceSkolemfunctions ( ?interim ?skolem)
}
<=
{
?graphlist rdf:first ?first.

_:x e:trace "kk".

?first e:graphList ?flist.
_:x e:findall ( { (?s ?p) e:skolem ?o } { ?triple list:in ?flist. ?triple log:equalTo {?s ?p ?o}. ?o a var:LocalVariable } ?slist  ).

(?interim ?slist) list:append ?interim2.


?graphlist rdf:rest ?rest.

?rest  :produceSkolemfunctions ( ?interim2 ?skolem)

}.
