

@prefix dash: <http://datashapes.org/dash#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/ns/example#> .




ex:mobilePhoneNumberShape a sh:NodeShape  ;
  sh:targetClass schema:Person ;
  sh:property ex:mobilePhoneNumberExistence .

ex:mobilePhoneNumberExistence 
  sh:path schema:contactPoint;
  sh:node ex:gsmShape;
  sh:minCount 1 .

ex:gsmShape a sh:NodeShape;
  sh:property [
              sh:path schema:contactType;
              sh:value "gsm"
              ];
  sh:property [
              sh:path schema:telephone;
              sh:minCount 1
              ]
.




ex:Ex
	a sh:NodeShape ;
	  sh:targetClass schema:Person ;
	sh:property [
		sh:path schema:contactPoint;
	        sh:minCount 1 ;
		sh:qualifiedValueShape [
			sh:path schema:contactType ;
			sh:hasValue "gsm" ;
		] ;
		sh:qualifiedMinCount 1 ;
	] .
